@model Spark.Store.Stats
<h2>Overview</h2>
The database of this server contains the following resources:
<h3>Resources</h3>
<table class="stats">
    @foreach (var stat in Model.ResourceStats)
    {
        <tr>
            <td><a href="@Spark.Config.Settings.Endpoint/@stat.ResourceName">@stat.ResourceName</a></td>
            <td>@stat.Count</td>
        </tr>
    }
</table>

